<p id="notice"><%= notice %></p>

<h1>Agreements</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agreements.each do |agreemnt| %>
      <tr>
        <td><%= link_to 'Show', agreemnt %></td>
        <td><%= link_to 'Edit', edit_agreement_path(agreemnt) %></td>
        <td><%= link_to 'Destroy', agreemnt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agreement', new_agreement_path %>
